<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.MainFragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageButton
        android:id="@+id/button10000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_margin="50dp"
        android:background="@null"
        app:srcCompat="?android:attr/textSelectHandleLeft"
        tools:layout_conversion_absoluteHeight="80dp"
        tools:layout_conversion_absoluteWidth="80dp"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="83dp" />

    <ImageButton
        android:id="@+id/button1000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_margin="50dp"
        android:layout_below="@id/button10000"
        android:background="@null"
        app:srcCompat="?android:attr/textSelectHandleLeft"
        tools:layout_conversion_absoluteHeight="80dp"
        tools:layout_conversion_absoluteWidth="80dp"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="203dp" />

    <ImageButton
        android:id="@+id/button100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_margin="50dp"
        android:layout_below="@id/button1000"
        android:background="@null"
        app:srcCompat="?android:attr/textSelectHandleLeft"
        tools:layout_conversion_absoluteHeight="80dp"
        tools:layout_conversion_absoluteWidth="80dp"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="324dp" />

    <ImageButton
        android:id="@+id/saveButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_margin="50dp"
        android:layout_below="@id/button100"
        android:background="@null"
        app:srcCompat="?android:attr/textSelectHandleLeft"
        tools:layout_conversion_absoluteHeight="80dp"
        tools:layout_conversion_absoluteWidth="80dp"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="444dp" />

    <TextView
        android:id="@+id/totalMoney"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/saveButton"
        android:layout_centerHorizontal="true"
        android:layout_margin="20dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="50dp"
        android:text="TextView"
        android:textSize="24sp"
        android:visibility="visible"
        tools:layout_conversion_absoluteHeight="32dp"
        tools:layout_conversion_absoluteWidth="98dp"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="544dp"
        tools:visibility="visible" />


    <TextView
        android:id="@+id/CurrentMoney"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/totalMoney"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:text="TextView"
        android:textSize="24sp"
        android:visibility="visible"
        tools:layout_conversion_absoluteHeight="32dp"
        tools:layout_conversion_absoluteWidth="98dp"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="577dp"
        tools:visibility="visible" />

</RelativeLayout>